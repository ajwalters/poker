h1 Game Show Test
h2 Game Id: #{gameId}
script(src="http://majek.github.com/sockjs-client/sockjs-latest.min.js")
script
  var sock = new SockJS(window.location.origin + '/game_socket/#{gameId}');
  sock.onopen = function() {
    console.log('open');
  };
  sock.onmessage = function(message) {
    console.log('received: ' + JSON.stringify(message.data));
    console.log('full message: ' + message);
  };
  sock.onclose = function(m) {
    console.log('closed',m.status,m.reason);
  };
